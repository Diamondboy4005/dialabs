    "dialabs:sniping_arrow_origin",
    "dialabs:sniping",
    "dialabs:sniping_speed_ref",
    "dialabs:sniping_divergence_ref"
  ],


  "mixin": "dialabs.mixins.json",
  "access_widener": "dialabs.accesswidener",
  "minecraft": {
    "environment": "*"
  }
